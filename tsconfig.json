{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@prisma/client": ["node_modules/@prisma/client"],
      "prismabox/*": ["prisma/prismabox/*"]
    }
  },
  "include": [
    "src",
    "src/types",
    "prisma/prismabox",
    "node_modules/@prisma/client"
  ]
}
